{
  "name": "nestjs-boilerplate",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "bin": "build/src/main.js",
  "pkg": {
    "scripts": "build/**/*.js",
    "assets": [
      "build/**/*"
    ],
    "targets": [
      "node16"
    ]
  },
  "prisma": {
    "seed": "ts-node -r tsconfig-paths/register prisma/seed.ts"
  },
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "NODE_ENV=development nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/src/main",
    "start:migrate:dev": "prisma migrate dev && nest start --watch",
    "start:mongo:dev": "prisma db push && ts-node-dev -r tsconfig-paths/register --respawn --ignore-watch node_modules --no-notify src/index.ts",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.config.ts",
    "prepare": "husky install",
    "doc": "npm run doc:serve",
    "doc:generate": "compodoc -p ./tsconfig.doc.json --theme readthedocs",
    "doc:serve": "compodoc -s -r 8090 --theme readthedocs",
    "doc:watch": "compodoc -p ./tsconfig.doc.json -w -s -r 8090 --theme readthedocs"
  },
  "dependencies": {
    "@nestjs/common": "^10.1.0",
    "@nestjs/core": "^10.1.0",
    "@nestjs/platform-express": "^10.1.0",
    "@nestjs/jwt": "10.1.0",
    "@nestjs/microservices": "10.1.0",
    "@nestjs/passport": "10.0.0",
    "@nestjs/serve-static": "^4.0.0",
    "@nestjs/swagger": "^7.1.2",
    "@nestjs/throttler": "^3.1.0",
    "@prisma/client": "5.1.1",
    "bcrypt": "5.0.1",
    "rate-limiter-flexible": "^2.4.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "config": "^3.3.7",
    "cookie-parser": "^1.4.6",
    "dd-trace": "4.15.0",
    "helmet": "^5.1.1",
    "ioredis": "^5.0.5",
    "passport": "0.6.0",
    "passport-local": "1.0.0",
    "nest-winston": "^1.6.2",
    "nestjs-i18n": "^10.2.6",
    "nestjs-throttler-storage-redis": "^0.1.18",
    "reflect-metadata": "0.1.13",
    "rimraf": "3.0.2",
    "rxjs": "^7.2.0",
    "ua-parser-js": "^1.0.32",
    "uuid": "8.3.2",
    "winston": "^3.10.0",
    "winston-cloudwatch": "^6.2.0"
  },
  "devDependencies": {
    "@aws-sdk/client-cloudwatch-logs": "^3.0.0",
    "@commitlint/cli": "16.2.3",
    "@jridgewell/sourcemap-codec": "^1.4.14",
    "@nestjs/cli": "^10.1.10",
    "@nestjs/schematics": "^10.0.1",
    "@nestjs/testing": "^10.1.3",
    "@rocketseat/commitlint-config": "0.0.3",
    "@types/connect-redis": "0.0.19",
    "@types/cookie-parser": "^1.4.2",
    "@types/express": "^4.17.13",
    "@types/jest": "29.2.4",
    "@types/multer": "^1.4.7",
    "@types/node": "20.3.1",
    "@types/supertest": "^2.0.11",
    "@types/passport-local": "1.0.34",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "connect-redis": "^6.0.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "7.0.4",
    "jest": "29.3.1",
    "prettier": "2.4.0",
    "prisma": "5.1.1",
    "redis": "^3.1.2",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "29.0.3",
    "ts-loader": "^9.3.1",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "4.1.1",
    "typescript": "^4.7.4",
    "webpack": "5.75.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
